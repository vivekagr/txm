{
  "name": "txm",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "eslint": "eslint ./src",
    "typegen": "apollo client:codegen --target=typescript --endpoint=http://localhost:5433/graphql --outputFlat ./src/data/types",
    "typegen:watch": "apollo client:codegen --target=typescript --endpoint=http://localhost:5433/graphql --outputFlat --watch ./src/data/types",
    "typecheck": "tsc --noEmit",
    "prettier:check": "prettier --check src/",
    "prettier:write": "prettier --write src/",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3",
    "@rollup/plugin-commonjs": "^15",
    "@rollup/plugin-node-resolve": "^9",
    "@rollup/plugin-typescript": "^6",
    "@tsconfig/svelte": "^1",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "apollo": "^2",
    "eslint": "^7",
    "eslint-config-airbnb-typescript": "^12",
    "eslint-config-prettier": "^6",
    "eslint-import-resolver-alias": "^1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-prettier": "^3",
    "prettier": "2",
    "prettier-plugin-svelte": "^1",
    "rollup": "^2",
    "rollup-plugin-livereload": "^2",
    "rollup-plugin-svelte": "^6",
    "rollup-plugin-terser": "^7",
    "svelte-check": "^1",
    "svelte-preprocess": "^4",
    "tslib": "^2",
    "typescript": "^4"
  },
  "dependencies": {
    "@apollo/client": "^3",
    "@tailwindcss/custom-forms": "^0.2",
    "chrono-node": "^2",
    "graphql": "^15",
    "sirv-cli": "^1",
    "svelte": "^3",
    "svelte-apollo": "^0.4",
    "svelte-spa-router": "^3",
    "tailwindcss": "^1.9",
    "xlsx": "^0.16"
  }
}
